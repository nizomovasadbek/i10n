#: json-glib/json-glib-format.c:58
msgid "Prettify output"
msgstr "Prettify chiqish"

#: json-glib/json-glib-format.c:59
msgid "Indentation spaces"
msgstr "Chiziq bo'shliqlari"

#: json-glib/json-glib-format.c:59
msgid "SPACES"
msgstr "JOYLAR"

#: json-glib/json-glib-format.c:60
msgid "Output file"
msgstr "Chiqish fayli"

#: json-glib/json-glib-format.c:60 json-glib/json-glib-validate.c:115
msgid "FILE"
msgstr "FAYL"

#: json-glib/json-glib-format.c:61
msgid "FILE…"
msgstr "FAYL…"

#. Translators: the first %s is the program name, the second one
#. * is the URI of the file, the third is the error message.
#.
#: json-glib/json-glib-format.c:87 json-glib/json-glib-format.c:123
#: json-glib/json-glib-validate.c:63
#, c-format
msgid "%s: %s: error opening file: %s\n"
msgstr "%s: %s: faylni ochishda xatolik: %s\n"

#. Translators: the first %s is the program name, the second one
#. * is the URI of the file, the third is the error message.
#.
#: json-glib/json-glib-format.c:101 json-glib/json-glib-validate.c:75
#, c-format
msgid "%s: %s: error parsing file: %s\n"
msgstr "%s: %s: faylni tahlil qilishda xatolik: %s\n"

#. Translators: the first %s is the program name, the
#. * second one is the URI of the file.
#.
#: json-glib/json-glib-format.c:143
#, c-format
msgid "%s: %s: error writing to stdout"
msgstr "%s: %s: stdout-ga yozishda xatolik"

#. Translators: the first %s is the program name, the second one
#. * is the URI of the file, the third is the error message.
#.
#: json-glib/json-glib-format.c:167 json-glib/json-glib-validate.c:87
#, c-format
msgid "%s: %s: error closing: %s\n"
msgstr "%s: %s: yopilish xatosi: %s\n"

#. Translators: this message will appear after the usage string
#. and before the list of options.
#: json-glib/json-glib-format.c:203
msgid "Format JSON files."
msgstr "JSON fayllarini formatlash."

#: json-glib/json-glib-format.c:204
msgid "json-glib-format formats JSON resources."
msgstr "json-glib-format formatlari JSON resurslari."

#. Translators: the %s is the program name. This error message
#. * means the user is calling json-glib-validate without any
#. * argument.
#.
#: json-glib/json-glib-format.c:219 json-glib/json-glib-validate.c:136
#, c-format
msgid "Error parsing commandline options: %s\n"
msgstr "Buyruqlar qatori parametrlarini tahlil qilishda xatolik yuz berdi: %s\n"

#: json-glib/json-glib-format.c:221 json-glib/json-glib-format.c:235
#: json-glib/json-glib-validate.c:138 json-glib/json-glib-validate.c:152
#, c-format
msgid "Try “%s --help” for more information."
msgstr "Qoʻshimcha maʼlumot olish uchun “%s --help”ni sinab koʻring."

#. Translators: the %s is the program name. This error message
#. * means the user is calling json-glib-validate without any
#. * argument.
#.
#: json-glib/json-glib-format.c:233 json-glib/json-glib-validate.c:150
#, c-format
msgid "%s: missing files"
msgstr "%s: yetishmayotgan fayllar"

#. Translators: this message will appear after the usage string
#. and before the list of options.
#: json-glib/json-glib-validate.c:118
msgid "Validate JSON files."
msgstr "JSON fayllarini tasdiqlang."

#: json-glib/json-glib-validate.c:119
msgid "json-glib-validate validates JSON data at the given URI."
msgstr "json-glib-validate berilgan URI da JSON ma'lumotlarini tasdiqlaydi."

#. translators: the %s is the name of the data structure
#: json-glib/json-gobject.c:924
#, c-format
msgid "Expecting a JSON object, but the root node is of type “%s”"
msgstr "JSON obyekti kutilmoqda, lekin root element “%s” turiga ega"

#. translators: the '%s' is the type name
#: json-glib/json-gvariant.c:482
#, c-format
msgid "Unexpected type “%s” in JSON node"
msgstr "JSON elementida kutilmagan “%s” turi"

#: json-glib/json-gvariant.c:552
msgid "Missing elements in JSON array to conform to a tuple"
msgstr "Kortejga mos keladigan JSON massivida eytishmayotgan elementlar"

#: json-glib/json-gvariant.c:580
msgid "Missing closing symbol “)” in the GVariant tuple type"
msgstr "GVariant korteji turidagi yopish belgisi “)” yetishmayapti"

#: json-glib/json-gvariant.c:588
msgid "Unexpected extra elements in JSON array"
msgstr "JSON massivida kutilmagan qo'shimcha elementlar"

#: json-glib/json-gvariant.c:889
msgid "Invalid string value converting to GVariant"
msgstr "Yaroqsiz satr qiymati GVariantga aylantirilmoqda"

#: json-glib/json-gvariant.c:944
msgid ""
"A GVariant dictionary entry expects a JSON object with exactly one member"
msgstr ""
"GVariant lug'at yozuvi aynan bitta a'zoga ega JSON obyektini kutadi"

#: json-glib/json-gvariant.c:1222
#, c-format
msgid "GVariant class “%c” not supported"
msgstr "GVariant klassi “%c” qo‘llab-quvvatlanmaydi"

#: json-glib/json-gvariant.c:1276
msgid "Invalid GVariant signature"
msgstr "GVariant imzosi yaroqsiz"

#: json-glib/json-gvariant.c:1327
msgid "JSON data is empty"
msgstr "JSON maʼlumotlari boʻsh"

#. translators: %s: is the file name, the first %d is the line
#. * number, the second %d is the position on the line, and %s is
#. * the error message
#.
#: json-glib/json-parser.c:939
#, c-format
msgid "%s:%d:%d: Parse error: %s"
msgstr "%s:%d:%d: Tahlil xatosi: %s"

#: json-glib/json-parser.c:1023
msgid "JSON data must be UTF-8 encoded"
msgstr "JSON maʼlumotlari UTF-8 bilan kodlangan boʻlishi kerak"

#: json-glib/json-path.c:389
msgid "Only one root node is allowed in a JSONPath expression"
msgstr "JSONPath ifodasida faqat bitta root elementiga ruxsat berilgan"

#. translators: the %c is the invalid character
#: json-glib/json-path.c:398
#, c-format
msgid "Root node followed by invalid character “%c”"
msgstr "Root elementidan keyin yaroqsiz “%c” belgisi"

#: json-glib/json-path.c:438
msgid "Missing member name or wildcard after . character"
msgstr "dan keyin aʼzo nomi yoki wildcard belgisi yoʻq. xarakter"

#: json-glib/json-path.c:512
#, c-format
msgid "Malformed slice expression “%*s”"
msgstr "“%*s” slice ifodasi noto‘g‘ri"

#: json-glib/json-path.c:556
#, c-format
msgid "Invalid set definition “%*s”"
msgstr "“%*s” taʼrifi notoʻgʻri"

#: json-glib/json-path.c:609
#, c-format
msgid "Invalid slice definition “%*s”"
msgstr "“%*s” Massiv indeksiga ko'rsatma berishda xatolik"

#: json-glib/json-path.c:637
#, c-format
msgid "Invalid array index definition “%*s”"
msgstr "“%*s” Massiv indeksini ko'rsatishda xatolik"

#: json-glib/json-path.c:656
#, c-format
msgid "Invalid first character “%c”"
msgstr "“%c” birinchi belgisi noto‘g‘ri"

#: json-glib/json-reader.c:504
#, c-format
msgid ""
"The current node is of type “%s”, but an array or an object was expected."
msgstr ""
"Joriy element “%s” turida, lekin massiv yoki obyekt kutilgan edi."

#: json-glib/json-reader.c:516
#, c-format
msgid ""
"The index “%d” is greater than the size of the array at the current position."
msgstr ""
"“%d” indeksi joriy pozitsiyadagi massiv hajmidan kattaroq."

#: json-glib/json-reader.c:533
#, c-format
msgid ""
"The index “%d” is greater than the size of the object at the current "
"position."
msgstr ""
"“%d” indeksi hozirgi vaqtda obyekt hajmidan kattaroq"
"pozitsiya".

#: json-glib/json-reader.c:616 json-glib/json-reader.c:789
#: json-glib/json-reader.c:838 json-glib/json-reader.c:877
#: json-glib/json-reader.c:917 json-glib/json-reader.c:957
#: json-glib/json-reader.c:997 json-glib/json-reader.c:1044
#: json-glib/json-reader.c:1082 json-glib/json-reader.c:1110
msgid "No node available at the current position"
msgstr "Joriy holatda hech qanday element mavjud emas"

#: json-glib/json-reader.c:623
#, c-format
msgid "The current position holds a “%s” and not an array"
msgstr "Joriy pozitsiya massiv emas, balki “%s” ni egallaydi"

#: json-glib/json-reader.c:705
#, c-format
msgid "The current node is of type “%s”, but an object was expected."
msgstr "Joriy element ”%s” turida, lekin obyekt kutilgan edi."

#: json-glib/json-reader.c:712
#, c-format
msgid "The member “%s” is not defined in the object at the current position."
msgstr "“%s” aʼzosi joriy holatda obyektda aniqlanmagan."

#: json-glib/json-reader.c:796 json-glib/json-reader.c:845
#, c-format
msgid "The current position holds a “%s” and not an object"
msgstr "Joriy pozitsiya obyekt emas, balki ”%s” ni egallaydi"

#: json-glib/json-reader.c:886 json-glib/json-reader.c:926
#: json-glib/json-reader.c:966 json-glib/json-reader.c:1006
#: json-glib/json-reader.c:1053
#, c-format
msgid "The current position holds a “%s” and not a value"
msgstr "Joriy pozitsiyada qiymat emas, balki “%s” mavjud"

#: json-glib/json-reader.c:1014
msgid "The current position does not hold a string type"
msgstr "Joriy pozitsiya string turiga ega emas"
