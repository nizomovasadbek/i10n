#: data/org.gnome.Tour.metainfo.xml.in.in:8
msgid "GNOME Tour and Greeter"
msgstr "GNOME Tour va Greeter"

#: data/org.gnome.Tour.metainfo.xml.in.in:10
msgid "A guided tour and greeter for GNOME."
msgstr "GNOME uchun ekskursiya va Greeter"

#: data/org.gnome.Tour.metainfo.xml.in.in:41
msgid "The GNOME Project"
msgstr "GNOME loyihasi"

#: data/resources/ui/paginator.ui:27
msgid "Previous"
msgstr "Oldingi"

#: data/resources/ui/paginator.ui:40
msgid "Next"
msgstr "Keyingisi"

#: data/resources/ui/paginator.ui:53
msgid "Start"
msgstr "Boshlash"

#: data/resources/ui/window.ui:11
msgid "Start the Tour"
msgstr "Sayohatni boshlang"

#: data/resources/ui/window.ui:17
msgid "Get an Overview"
msgstr "Umumiy koʻrinishga ega boʻling"

#: data/resources/ui/window.ui:18
msgid "Press the Super key to see open windows and apps."
msgstr "Ochiq oyna va ilovalarni ko'rish uchun Super tugmachasini bosing."

#: data/resources/ui/window.ui:24
msgid "Just Type to Search"
msgstr "Qidirish uchun shunchaki yozing"

#: data/resources/ui/window.ui:25
msgid "Type in the overview to search. Launch apps, find things."
msgstr "Qidirish uchun umumiy koʻrinishni kiriting. Ilovalarni ishga tushiring, narsalarni toping."

#: data/resources/ui/window.ui:31
msgid "Keep on Top with Workspaces"
msgstr "Workspaces bilan yuqorida turing"

#: data/resources/ui/window.ui:32
msgid "Easily organize windows with the workspaces view."
msgstr "Ish joylari ko'rinishida oynalarni osongina tartibga soling."

#: data/resources/ui/window.ui:38
msgid "Up/Down for the Overview"
msgstr "Umumiy koʻrinish uchun yuqoriga/pastga"

#: data/resources/ui/window.ui:39
msgid "On a touchpad, use three-finger vertical swipes. Try it!"
msgstr "Sensorli panelda uch barmoq bilan vertikal surishlardan foydalaning. Urunib ko'r!"

#: data/resources/ui/window.ui:45
msgid "Left/Right for Workspaces"
msgstr "Ish joylari uchun chap/o‘ng"

#: data/resources/ui/window.ui:46
msgid "On a touchpad, use three-finger horizontal swipes. Try it!"
msgstr "Sensorli panelda uch barmoq bilan gorizontal surishlardan foydalaning. Urunib ko'r!"

#: data/resources/ui/window.ui:52
msgid "That's it. Have a nice day!"
msgstr "Bo'ldi shu. Hayrli kun!"

#: data/resources/ui/window.ui:53
msgid "To get more advice and tips, see the Help app."
msgstr "Qoʻshimcha maslahat va maslahatlar olish uchun Yordam ilovasiga qarang."

#. Translators: The following string is formated as "Learn about new and essential features in GNOME 3.36" for example
#: src/widgets/window.rs:52
msgid "Learn about the key features in {name} {version}."
msgstr "{name} {version} ning asosiy xususiyatlari haqida bilib oling."
